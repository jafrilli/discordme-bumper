{  
    "manifest_version": 2,
    "version": "1.0.0",
    "name": "discord.me bumper",
    "author": "Jafar Ali",
    "description": "bump my shit",
    "background": {
      "persistent": false,
      "scripts": ["background.js"]
    },
    "permissions": [
        "storage",
        "tabs",
        "<all_urls>"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    }
}
